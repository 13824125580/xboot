{
	"led-gpio@0": {
		"gpio": 0,
		"active-low": true,
		"default-brightness": 0
	},

	"led-gpio@1": {
		"gpio": 1,
		"active-low": true,
		"default-brightness": 0
	},

	"rotary-encoder": {
		"gpio-a": 2,
		"gpio-b": 3,
		"gpio-c": 4,
		"inverted-a": false,
		"inverted-b": false,
		"inverted-c": false,
		"step-per-period": 1
	},

	"rc-gpio": {
		"gpio": 5,
		"active-low": true,
		"maps": [
			{ "scan-code": 123, "key-code": 56 },
			{ "scan-code": 124, "key-code": 88 }
		]
	},

	"buzzer-gpio": {
		"gpio": 6,
		"active-low": true
	},

	"vibrator-gpio": {
		"gpio": 7,
		"active-low": true
	},

	"rtc-pl031@0x10017000": {
	}
}
